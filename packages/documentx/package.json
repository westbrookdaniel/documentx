{
    "name": "documentx",
    "version": "0.10.0",
    "type": "module",
    "license": "MIT",
    "author": "Daniel Westbrook",
    "keywords": [
        "framework",
        "documentx"
    ],
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime.js",
            "types": "./dist/jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx-dev-runtime.js",
            "types": "./dist/jsx-dev-runtime.d.ts"
        },
        "./package.json": "./package.json",
        "./dist/*": "./dist/*"
    },
    "homepage": "https://github.com/westbrookdaniel/documentx#readme",
    "scripts": {
        "build": "microbundle -f modern src/jsx-runtime.ts src/jsx-dev-runtime.ts src/index.ts -o dist/index.js"
    },
    "devDependencies": {
        "@types/node": "^20.1.3",
        "cac": "^6.7.14",
        "cross-fetch": "^3.1.5",
        "microbundle": "^0.15.1",
        "tsconfig-documentx": "workspace:*",
        "typescript": "5.1.0-beta"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "esbuild": "^0.19.5",
        "glob": "^10.3.10",
        "h3": "^1.8.2",
        "linkedom": "^0.16.1",
        "listhen": "^1.5.5",
        "vite": "^4.5.0"
    }
}
