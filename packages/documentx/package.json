{
    "name": "documentx",
    "version": "0.2.5",
    "type": "module",
    "license": "MIT",
    "author": "Daniel Westbrook",
    "keywords": [
        "frontend",
        "framework",
        "documentx"
    ],
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "jsx-runtime.d.ts",
        "jsx-dev-runtime.d.ts"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime.js",
            "require": "./dist/jsx-runtime.cjs",
            "types": "./jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx-dev-runtime.js",
            "require": "./dist/jsx-dev-runtime.cjs",
            "types": "./jsx-dev-runtime.d.ts"
        },
        "./package.json": "./package.json",
        "./dist/*": "./dist/*"
    },
    "homepage": "https://github.com/westbrookdaniel/documentx#readme",
    "scripts": {
        "build": "vite build && mv ./dist/jsx-runtime.d.ts ./jsx-runtime.d.ts && mv ./dist/jsx-dev-runtime.d.ts ./jsx-dev-runtime.d.ts"
    },
    "devDependencies": {
        "tsconfig-documentx": "workspace:*",
        "typescript": "^4.5.2",
        "vite": "^4.3.2",
        "vite-plugin-dts": "^2.3.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
