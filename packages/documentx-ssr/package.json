{
    "name": "documentx-ssr",
    "version": "0.1.0",
    "type": "module",
    "license": "MIT",
    "author": "Daniel Westbrook",
    "keywords": [
        "fullstack",
        "ssr",
        "framework",
        "documentx"
    ],
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "main": "./dist/index.cjs",
    "module": "./dist/index.module.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "documentx-ssr": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./plugin": {
            "import": "./dist/plugin.js",
            "require": "./dist/plugin.cjs",
            "types": "./dist/plugin.d.ts"
        },
        "./cli": {
            "import": "./dist/cli.js",
            "require": "./dist/cli.cjs",
            "types": "./dist/cli.d.ts"
        },
        "./package.json": "./package.json",
        "./dist/*": "./dist/*"
    },
    "homepage": "https://github.com/westbrookdaniel/documentx#readme",
    "scripts": {
        "build": "microbundle --target node -f modern,esm,cjs src/*.ts"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.1.3",
        "microbundle": "^0.15.1",
        "tsconfig-documentx": "workspace:*",
        "typescript": "5.1.0-beta",
        "documentx": "workspace:*",
        "vite": "^4.3.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "documentx": "workspace:*",
        "vite": "^4.3.2"
    },
    "dependencies": {
        "cac": "^6.7.14",
        "cross-fetch": "^3.1.5",
        "express": "^4.18.2"
    }
}
